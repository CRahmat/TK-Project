</head>
<body>
    <main>
        <!-- Header -->
        <header id="js-header" class="u-header u-header--sticky-top u-header--toggle-section u-header--change-appearance" data-header-fix-moment="300">
            <div class="u-header__section u-header__section--light g-bg-white g-transition-0_3 g-py-8" data-header-fix-moment-exclude="g-py-8" data-header-fix-moment-classes="u-shadow-v18 g-py-0">
                <nav class="navbar navbar-expand-lg">
                    <div class="container">
                        <!-- Responsive Toggle Button -->
                        <button class="navbar-toggler navbar-toggler-right btn g-line-height-1 g-brd-none g-pa-0 g-pos-abs g-top-3 g-right-0" type="button" aria-label="Toggle navigation" aria-expanded="false" aria-controls="navBar" data-toggle="collapse" data-target="#navBar">
                            <span class="hamburger hamburger--slider">
                                <span class="hamburger-box">
                                    <span class="hamburger-inner"></span>
                                </span>
                            </span>
                        </button>
                        <!-- End Responsive Toggle Button -->
                        <!-- Logo -->
                        <a href="@Url.Action("Index", "Home")" class="navbar-brand">
                            <img width="170" src="~/assets/images/logo/logo-header.png" alt="Image Description">
                        </a>
                        <!-- End Logo -->
                        <!-- Navigation -->
                        <div class="collapse navbar-collapse align-items-center flex-sm-row g-pt-10 g-pt-5--lg" id="navBar">
                            <ul id="js-scroll-nav" class="navbar-nav text-uppercase g-font-weight-700 g-font-size-11 g-pt-20 g-pt-0--lg ml-auto">
                                <li class="nav-item g-mx-20--lg">
                                    <a href="@Url.Action("Index", "Home")" class="js-go-to nav-link px-0 active">
                                        Home <span class="sr-only">(current)</span>
                                    </a>
                                </li>
                                <li class="nav-item g-mx-20--lg">
                                    <a href="" data-target="#about-section" class="js-go-to nav-link px-0">
                                        Tentang
                                    </a>
                                </li>
                                <li class="nav-item g-mx-20--lg">
                                    <a href="" data-target="#promo-section" class="js-go-to nav-link  px-0">
                                        Visi Misi
                                    </a>
                                </li>
                                <li class="nav-item g-mx-20--lg">
                                    <a href="" data-target="#product-section" class="js-go-to nav-link px-0">
                                        Galeri
                                    </a>
                                </li>
                                <li class="nav-item g-mx-20--lg">
                                    <a href="" data-target="#contact-section" class="js-go-to nav-link px-0">
                                        Kontak Kami
                                    </a>
                                </li>
                                @if (User.Identity.Name == "" || User.Identity.Name == null)
                                {
                            <li class="nav-item g-mx-20--lg">
                                <a href="@Url.Action("Login", "Account")" class="nav-link px-0">
                                    Login
                                </a>
                            </li>
 }
                        else
                        {
                            <li class="nav-item g-mx-20--lg">
                                <a href="@Url.Action("Profile", "Home")" class="nav-link px-0">
                                    Profil
                                </a>
                            </li>
                                            <li class="nav-item g-mx-20--lg">
                                                <a href="@Url.Action("LogOff", "Account")" class="nav-link px-0">
                                                    Log Out
                                                </a>
                                            </li>
}
                            </ul>
                        </div>
                        <!-- End Navigation -->
                    </div>
                </nav>
            </div>
        </header>
        <!-- End Header -->
        @RenderBody()
        <footer class="g-bg-black-opacity-0_9 g-color-white-opacity-0_8 g-py-20">
            <div class="container">
                <!-- Copyright -->
                <div class="row justify-content-center text-center">
                    <div class="col-md-12 g-mb-15 g-mb-0--md">
                        <small class="g-font-size-default">
                            2021 All right reserved. Development by
                            <a class="g-color-white" href="#">KBTKIT Mutiara Hati Pedan</a>
                        </small>
                    </div>

                    @*<div class="col-md-6">
                            <ul class="list-inline text-md-right mb-0">
                                <li class="list-inline-item">
                                    <a class="u-icon-v2 u-icon-size--xs g-color-white-opacity-0_8 g-color-white--hover g-bg-primary--hover g-brd-white-opacity-0_2 g-brd-primary--hover rounded" href="#">
                                        <i class="fa fa-twitter"></i>
                                    </a>
                                </li>
                                <li class="list-inline-item">
                                    <a class="u-icon-v2 u-icon-size--xs g-color-white-opacity-0_8 g-color-white--hover g-bg-primary--hover g-brd-white-opacity-0_2 g-brd-primary--hover rounded" href="#">
                                        <i class="fa fa-facebook"></i>
                                    </a>
                                </li>
                                <li class="list-inline-item">
                                    <a class="u-icon-v2 u-icon-size--xs g-color-white-opacity-0_8 g-color-white--hover g-bg-primary--hover g-brd-white-opacity-0_2 g-brd-primary--hover rounded" href="#">
                                        <i class="fa fa-linkedin"></i>
